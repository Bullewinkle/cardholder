#app
	position relative
	width 100%
	height 100%	
	height 0

#printableCardsGreed
	display block !important
	position absolute
	top 0
	left 0
	width 100%
	background red
	&.printable-images
		display inline-block
		border 2px solid rgba(0,0,0,0.5)
		width 22.8%
		margin 1%

